{
    "name": "Bytedance-UA",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "设置UA",
    "homepage_url": "https://ad.toutiao.com/adver/",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "options_page": "options.html",
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon19.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["content.css"],
        "js": ["lib/jquery.js", "lib/helper.js", "lib/myslow.js", "performance.js", "contentscript.js"]
    }],
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "tabs",
        "storage",
        "cookies"
    ]
}
